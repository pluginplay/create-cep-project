{
  "name": "ae-plugin",
  "version": "0.0.1",
  "description": "An After Effects plugin.",
  "extendScriptClass": "AEPlugin",
  "manifest": {
    "bundleName": "AE Plugin",
    "bundleId": "app.pluginplay.ae-plugin",
    "hosts": {
      "AEFT": "13.0"
    },
    "requiredRuntimes": {
      "CSXS": "5.0"
    },
    "extensions": {
      "panel": {
        "title": "AE Plugin",
        "size": [
          300,
          300
        ],
        "minSize": [
          210,
          200
        ]
      }
    }
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "neutrino build",
    "start": "neutrino start",
    "lint": "neutrino lint",
    "test": "neutrino test",
    "release": "standard-version -r",
    "deploy-local": "cep-scripts deploy-local",
    "package": "cep-scripts package",
    "publish-release": "cep-scripts release"
  },
  "devDependencies": [
    "@neutrinojs/jest@^8",
    "@neutrinojs/standardjs@^8",
    "@pluginplay/cep-scripts",
    "cep-lib",
    "chai@^4",
    "chai-jest-diff@^1",
    "dirty-chai@^2",
    "jest@^23",
    "jest-junit@^5",
    "jest-styled-components@^6",
    "mobx@^4",
    "mobx-react@^5",
    "neutrino@^8",
    "neutrino-preset-cep",
    "prop-types@^15",
    "react@^16",
    "react-cep",
    "react-dom@^16",
    "react-hot-loader@^4",
    "standard-version@^4",
    "styled-components@^3",
    "styled-flex-component@^2"
  ]
}
